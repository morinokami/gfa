		"@types/json-stable-stringify": "^1.0.36",
		"@types/node": "22.7.7",
		"knip": "5.30.6",
		"pkg-pr-new": "0.0.20",
		"tsx": "4.17.0",
		"typescript": "5.6.2"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "0.0.51",
		"@ai-sdk/openai": "0.0.44",
		"@hono/node-server": "1.12.2",
		"ai": "3.4.16",
		"chalk": "5.3.0",
		"hono": "4.6.1",
		"json-stable-stringify": "^1.1.1",
		"meow": "13.2.0",
		"ora": "8.0.1",
		"vite": "6.0.7",
