		"knip": "5.27.2",
		"pkg-pr-new": "0.0.19",
		"tsx": "4.16.5",
		"typescript": "5.5.4"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "0.0.38",
		"@ai-sdk/openai": "0.0.43",
		"@hono/node-server": "1.12.0",
		"ai": "3.3.3",
		"chalk": "5.3.0",
		"hono": "4.5.3",
		"meow": "13.2.0",
		"ora": "8.0.1",
		"vite": "5.3.5",
		"zod": "3.23.8"
	},
	"peerDependencies": {
		"zod": "^3"
	}
